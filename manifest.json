{
    "manifest_version": 2,
    "name": "KQL",
    "description": "Kinvey Fuzzy Finder Chrome extension",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
